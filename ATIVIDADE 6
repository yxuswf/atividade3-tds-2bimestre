programa
{
	
	funcao inicio()
	{
	
    inteiro n1, n2, n3, n4, maior
    
    escreva("Informe o número 1: ")
    leia(n1)
    
    escreva("Informe o número 2: ")
    leia(n2)
    
    escreva("Infome o número 3: ")
    leia(n3)
    
    escreva("Informe o número 4: ")
    leia(n4)
   
    maior = n1

    se (n2 > maior)
        maior = n2

    se (n3 > maior)
        maior = n3

    se (n4 > maior)
        maior = n4
   
    escreva("O maior número é: ", maior)


	}
}
