programa
{
  funcao inicio()
  {
    inteiro nota1, nota2, media

    escreva("Informe a primeira nota: ")
    leia(nota1)
    escreva("Informe a segunda nota: ")
    leia(nota2)

    media = (nota1 + nota2) / 2

    se (media == 10) {
      escreva("Aprovado com Distinção!")
    } 
   		senao se (media >= 7) {
      		escreva("Aprovado!")
    		} 
    			senao {
     			escreva("Reprovado!")
   			}
  }
}
